{
    "compilerOptions": {
      "jsx": "react-jsx",
      "target": "es2020",
      "module": "commonjs",
      "moduleResolution": "node",
      "allowJs": true,
      "checkJs": true,
      "baseUrl": "src",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "paths": {
        "*": ["*"]
      }
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "build"]
  }